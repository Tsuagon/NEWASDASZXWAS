	mouse.KeyDown:connect(function(key)
	if key == "e" then
		if canattack == true then
			if punched1 == true then
				punched1 = false
				punched3 = false
				punched4 = false
				canidle = false
				canwalk= false
				
				firsthit()
				canidle = true
				canwalk= true
				punched2 = true
			end
		end
	end
	end)
